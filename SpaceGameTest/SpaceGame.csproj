<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net8.0-windows</TargetFramework> <!-- Windows-specific target -->
    <UseWPF>false</UseWPF> <!-- You are not using WPF -->
    <UseWindowsForms>true</UseWindowsForms> <!-- You need Windows Forms -->
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
  </PropertyGroup>
</Project>
